#payServices {
    width: 100%;
    padding: 140px 0;
    background-image: url(https://images.pexels.com/photos/767956/pexels-photo-767956.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940);
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size: cover;
}

    #payServices .card1 {
        position: relative;
        max-width: 300px;
        background: rgba(0, 0, 0, 0.56);
        margin: 0 auto;
        padding: 40px 20px;
        transition: .5s;
        text-align: center;
        border-radius: 7px;
        border: 1px solid #fff;
        transition: all .5s linear;
    }

        #payServices .card1:hover {
            box-shadow: -1px -3px 18px white;
            transform: scale(1.1)
        }



    #payServices .card2 {
        position: relative;
        max-width: 500px;
        height: 680px;
        background: rgba(0, 0, 0, 0.56);
        margin: -65px auto;
        padding: 40px 20px;
        transition: .5s;
        text-align: center;
        border-radius: 7px;
        border: 1px solid #fff;
    }

    #payServices .col-sm-4:nth-child(1).card1 {
        background: linear-gradient(-45deg, #f403d1, #64b5f6);
    }

    #payServices .card:before {
        content: '';
        position: absolute;
        border: 0;
        left: 0;
        width: 100%;
        height: 40%;
        background: rgba(255, 255, 255, 0.1);
        z-index: 2;
        transform: skewY(-5deg) scale(1.5);
    }

    #payServices .title {
        width: 100px;
        height: 100px;
        border-radius: 50%;
        margin-left: 30%;
        margin-bottom: 30px;
        line-height: 100px;
        font-size: 53px;
        box-shadow: 0 10px 10px #fff;
    }

        #payServices .title svg {
            color: #fff;
            font-size: 60px;
            margin-top: 20px;
        }

    #payServices .option {
        position: relative;
        z-index: 2;
    }

    #payServices .option2 {
        position: relative;
        z-index: 2;
    }

    #payServices .option ul {
        margin: 0;
        padding: 0;
    }

    #payServices .option2 ul {
        margin: 0;
        padding: 0;
    }

    #payServices .option ul li {
        margin: 0 0 10px;
        padding: 0;
        list-style: none;
        color: #fff;
        font-size: 16px;
        text-align: 20px;
    }

    #payServices .option2 ul li {
        margin: 0 0 10px;
        padding: 0;
        list-style: none;
        color: #fff;
        font-size: 19px;
        text-align: 20px;
    }

    #payServices .card1 h2 {
        color: #fff;
    }

    #payServices .card2 h2 {
        color: #fff;
    }

    #payServices .card1 h4 {
        color: #fff;
        padding: 20px 0;
        font-size: 3em;
    }

    #payServices .card2 h4 {
        color: #fff;
        padding: 20px 0;
        font-size: 5em;
    }

    #payServices .card1 a {
        position: relative;
        z-index: 2;
        background: #fff;
        color: black;
        width: 150px;
        height: 40px;
        line-height: 40px;
        border-radius: 7px;
        display: block;
        text-align: center;
        margin: 20px auto 0;
        font-size: 19px;
        cursor: pointer;
        box-shadow: 0 5px 10px rgba(0, 0, 0, 0.3);
        transition: all .4s linear;
    }

        #payServices .card1 a:hover {
            background: #fc0;
        }

    #payServices .card2 a {
        position: relative;
        z-index: 2;
        background: #fff;
        color: black;
        width: 150px;
        height: 40px;
        line-height: 40px;
        border-radius: 7px;
        display: block;
        text-align: center;
        margin: 95px auto 0;
        list-style: none;
        font-size: 19px;
        cursor: pointer;
        box-shadow: 0 5px 10px rgba(0, 0, 0, 0.3);
        transition: all .4s linear;
    }

        #payServices .card2 a:hover {
            background: #439288;
            color: white;
        }

    #payServices .card1 a:hover {
        text-decoration: none;
        background: #439288;
        color: white;
    }

    #payServices .card1 a:hover {
        text-decoration: none;
    }


